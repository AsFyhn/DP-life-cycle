    "fig.add_plot(model_real.grid_age[:-1], (sim_fit.C_avg), label=f\"Fitted values\", linestyle='-')\n",
    "fig.add_plot(model_real.grid_age[:-1], (sim_fit.Y_avg), label=f\"Income\", linestyle='-')\n",
